{
  "semi": false,
  "singleQuote": true,
  "tabWidth": 3,
  "trailingComma": "es5",
  "printWidth": 80,
  "bracketSpacing": true,
  "arrowParens": "avoid",
  "endOfLine": "lf",
  "quoteProps": "as-needed",
  "jsxSingleQuote": true,
  "bracketSameLine": false,
  "proseWrap": "preserve",
  "plugins": ["prettier-plugin-tailwindcss"]
}
